{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": false,
    "sourceMap": false,
    "inlineSources": false,
    "pretty": false,
    "outDir": "./dist"
  },
  "compileOnSave": true,
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "./node_modules/*"
  ]
}